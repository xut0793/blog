#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/blog/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/bin/node_modules:/mnt/f/blog/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/node_modules:/mnt/f/blog/node_modules/.pnpm/esbuild@0.14.23/node_modules:/mnt/f/blog/node_modules/.pnpm/node_modules:/mnt/f/blog/node_modules:/mnt/f/node_modules:/mnt/f/blog/node_modules/.pnpm/vite@2.8.4/node_modules/vite/node_modules:/mnt/f/blog/node_modules/.pnpm/vite@2.8.4/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/f/blog/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/bin/node_modules:/mnt/f/blog/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/node_modules:/mnt/f/blog/node_modules/.pnpm/esbuild@0.14.23/node_modules:/mnt/f/blog/node_modules/.pnpm/node_modules:/mnt/f/blog/node_modules:/mnt/f/node_modules:/mnt/f/blog/node_modules/.pnpm/vite@2.8.4/node_modules/vite/node_modules:/mnt/f/blog/node_modules/.pnpm/vite@2.8.4/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esbuild@0.14.23/node_modules/esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../../esbuild@0.14.23/node_modules/esbuild/bin/esbuild" "$@"
fi
